<?xml version="1.0" encoding="utf-8"?>
<com.mycompany.readmark.ui.widget.floatingactionbutton.MultiFloatingActionButton
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/floating_button"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    app:switchFabColor="@color/colorAccent"
    app:switchFabIcon="@mipmap/ic_add_white_48dp"
    app:layout_behavior="com.mycompany.readmark.ui.widget.floatingactionbutton.HideBehavior"
    app:animationMode="scale">
    <!-- app:layout_behavior="com.fjl.floatbutton.widget.FabBehavior"
    -->
    <com.mycompany.readmark.ui.widget.floatingactionbutton.TagFabLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:tagText="搜书"
        >
        <android.support.design.widget.FloatingActionButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@mipmap/ic_search_white_48dp"
            app:backgroundTint="#468cb7"
            app:fabSize="mini" />
    </com.mycompany.readmark.ui.widget.floatingactionbutton.TagFabLayout>

    <com.mycompany.readmark.ui.widget.floatingactionbutton.TagFabLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:tagText="查看书签"
        >
        <android.support.design.widget.FloatingActionButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@mipmap/ic_bookmark_border_white_48dp"
            app:backgroundTint="#818aa7"
            app:fabSize="mini" />
    </com.mycompany.readmark.ui.widget.floatingactionbutton.TagFabLayout>
<!--
    <com.mycompany.readmark.customview.FloatingActionButton.TagFabLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:tagText="Send mail"
        >
        <android.support.design.widget.FloatingActionButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@mipmap/ic_add_white_48dp"
            app:backgroundTint="#4BB7A7"
            app:fabSize="mini" />
    </com.mycompany.readmark.customview.FloatingActionButton.TagFabLayout>
-->
</com.mycompany.readmark.ui.widget.floatingactionbutton.MultiFloatingActionButton>